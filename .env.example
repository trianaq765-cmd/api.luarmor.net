# ============================================================
# SERVER CONFIGURATION
# ============================================================
PORT=3000
NODE_ENV=development

# ============================================================
# SECURITY KEYS (WAJIB GANTI DI PRODUCTION!)
# ============================================================
SECRET_KEY=change-this-to-a-very-long-random-string-min-32-chars
ADMIN_KEY=change-this-admin-key-to-something-secure

# ============================================================
# ORIGINAL SCRIPT URL
# ============================================================
ORIGINAL_SCRIPT_URL=https://api.junkie-development.de/api/v1/luascripts/public/8a56151af71ed4b56c346b2bef75d232f22d3ffb242e31d5ef79d12f69d974d6/download

# ============================================================
# RATE LIMITING
# ============================================================
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=10

# ============================================================
# SECURITY OPTIONS
# ============================================================
ENABLE_OBFUSCATION=true
ENABLE_ANTI_TAMPER=true
ENABLE_HEARTBEAT=true
MAX_FAILED_ATTEMPTS=5

# ============================================================
# CACHE SETTINGS
# ============================================================
SCRIPT_CACHE_TTL=300
